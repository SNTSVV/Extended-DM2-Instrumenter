<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="preferExternal" package="com.haringeymobile.ukweather">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <meta-data android:name="android.support.VERSION" android:value="25.3.1"/>
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher_weather" android:label="@string/app_name" android:name="com.haringeymobile.ukweather.WorldWeatherApplication" android:supportsRtl="true">
        <meta-data android:name="android.app.default_searchable" android:value="com.haringeymobile.ukweather.MainActivity"/>
        <activity android:label="@string/app_name" android:launchMode="singleTop" android:name="com.haringeymobile.ukweather.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/city_searchable"/>
        </activity>
        <activity android:label="@string/weather_info_activity_name" android:name="com.haringeymobile.ukweather.weather.WeatherInfoActivity" android:parentActivityName="com.haringeymobile.ukweather.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".MainActivity"/>
        </activity>
        <activity android:icon="@drawable/ic_home" android:label="@string/city_management_activity_name" android:name="com.haringeymobile.ukweather.CityManagementActivity" android:parentActivityName="com.haringeymobile.ukweather.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".MainActivity"/>
        </activity>
        <activity android:icon="@drawable/ic_settings" android:label="@string/mi_settings_title" android:name="com.haringeymobile.ukweather.settings.SettingsActivity" android:parentActivityName="com.haringeymobile.ukweather.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".MainActivity"/>
        </activity>
        <activity android:icon="@drawable/ic_action_book" android:label="@string/mi_about_program_title" android:name="com.haringeymobile.ukweather.AboutActivity" android:parentActivityName="com.haringeymobile.ukweather.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".MainActivity"/>
        </activity>
        <provider android:authorities="com.haringeymobile.ukweather.provider" android:exported="false" android:name="com.haringeymobile.ukweather.database.WeatherContentProvider"/>
        <service android:exported="false" android:name="com.haringeymobile.ukweather.database.GeneralDatabaseService"/>
    </application>
</manifest>